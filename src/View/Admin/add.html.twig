{% extends 'layout.html.twig' %}

{% block content %}
	<h1 class="text-center mt-3 mb-3">Ajouter une nouvelle musique</h1>
	{% if errors is not empty %}
		<div class="errors_messages">
			<ul>
				{% for error in errors %}
					<li>{{ error }}</li>
